{
  "name": "rollup-plugin-extensions",
  "version": "0.0.1",
  "description": "Allow rollup to resolve local files with any extension",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/types/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/zelzen/rollup-plugin-extensions"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "rollup-plugin",
    "rollup",
    "extension",
    "extensions",
    "resolve"
  ],
  "author": "Gabe Meola <gabe@zelzen.io>",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "build:types": "tsc -p . --emitDeclarationOnly",
    "prepublishOnly": "yarn build && yarn build:types"
  },
  "devDependencies": {
    "rollup": "^1.4.1",
    "rollup-typescript": "^1.2.0",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "rollup": "^1.4.1"
  }
}
